<div class="cfn_cont">
    <h2>Login as a Teacher</h2>
    <p class="inst">
        Enter the username for the teacher account you would like to access. Enter the email
        address only, do NOT include @schools.nyc.gov.
    </p>
    <div class="cfn_login hightAuto">
        <div>
            <h3>Teacher Name:</h3>
            <input name="ctl00$MainContent$CFNHome1$txtCentralAccountUserName" type="text" id="MainContent_CFNHome1_txtCentralAccountUserName" class="tbox" onkeypress="EnterEvent(event)" style="width:360px;" />
            <input type="submit" name="ctl00$MainContent$CFNHome1$btnLogin" value="Sign In" id="MainContent_CFNHome1_btnLogin" class="loginbutton" />
        </div>
        <div class="error-block">
            <label id="MainContent_CFNHome1_lblErrorMessage" class="errorMessage" style="color: red; display: none;"></label>
        </div>
    </div>

    
</div>
<script type="text/javascript">

    function EnterEvent(event) {
        if (event.keyCode == 13) {
            __doPostBack('btnLogin', '');
        }
    }

    $("#MainContent_CFNHome1_btnLogin").click(function () {

        var txtUserNameCtrl = $('#MainContent_CFNHome1_txtCentralAccountUserName');

        var lblErrorMessageCtrl = $('label[id ="MainContent_CFNHome1_lblErrorMessage"]');

        lblErrorMessageCtrl.html('');
        lblErrorMessageCtrl.css('display', '');

        if (txtUserNameCtrl.val().length === 0) {
            lblErrorMessageCtrl.html('Teacher Name is required.');
            lblErrorMessageCtrl.css('display', 'inline');
            return false;
        }
        else {
            return true;
        }
    });


</script>


