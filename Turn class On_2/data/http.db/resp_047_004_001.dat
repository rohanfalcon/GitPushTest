<script type="text/javascript">
    $(document).ready(function () {
        $("div.scheduled").on("mouseenter", function () {
            var pillElement = $(this),
            pillTooltipElement = $(this).children(".class_desc");
            pillTooltipElement.find(".bottom ul li").css({
                "margin": "10px 20px 0px 15px",
                "word-wrap": "break-word",
                "overflow-wrap": "break-word"
            });            
            pillTooltipElement.find(".bottom ul li div").css("margin", "0px");

            var pillHeight = pillElement.height(),
                pos_top = pillElement.offset().top + pillHeight,
                pos_bottom = $(document).height() - pos_top;

            if (pos_bottom < pillTooltipElement.height() + 30) {
                pillTooltipElement.css({ bottom: 110, top: "auto" });
                pillTooltipElement.animate({ opacity: "show" }, "fast");
            }
            else {
                pillTooltipElement.css({ bottom: "", top: "" });
                pillTooltipElement.animate({ opacity: "show" }, "fast");
            }
        });

        $("div.scheduled").on("mouseleave", function () {
            $(this).children(".class_desc").animate({ opacity: "hide" }, 0);
        });
    });
</script>


    
