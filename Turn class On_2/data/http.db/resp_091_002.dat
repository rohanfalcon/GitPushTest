    $(document).ready(function () {
        $(".message-board .message-board-close").click(function () {
            $(this).parent().parent().children(".messge-board-content").toggle("slow", function () {
                if ($(this).parent().find(".message-board-close").html() === "+"){
                    $(this).parent().find(".message-board-close").html("-");
                } else {
                    $(this).parent().find(".message-board-close").html("+");
                }
            });

        });
    });
